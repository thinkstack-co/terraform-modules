{
  "threshold": 4,
  "reporters": ["console", "json"],
  "ignore": [
    "**/node_modules/**",
    "**/.git/**",
    "**/package/**",
    "**/packages/**",
    "**/vendor/**",
    "**/.venv/**",
    "**/build/**",
    "**/dist/**",
    "**/*.min.js",
    "**/*.min.css",
    "**/.terraform/**",
    "**/lambda_package.zip",
    "**/*.pyc",
    "**/*.pyo",
    "**/.mypy_cache/**",
    "**/.pytest_cache/**",
    "**/ec2_instance/main.tf",
    "**/ec2_domain_controller/main.tf"
  ],
  "minLines": 5,
  "minTokens": 70,
  "output": "./jscpd-report",
  "silent": false,
  "blame": true,
  "format": ["python", "javascript", "typescript", "terraform", "yaml", "json", "bash", "dockerfile"]
}
